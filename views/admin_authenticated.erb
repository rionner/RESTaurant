<html>
  <head>
    <link rel='stylesheet' href='/stylesheets/styles.css' media='screen'>
    <link href='http://fonts.googleapis.com/css?family=Martel+Sans:300' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=IM+Fell+English+SC' rel='stylesheet' type='text/css'>
    <%= erb :template %>
  </head>
  <body>
    <nav>
      <div class="logo">
        <h1>Munch</h1>
      </div>
      <ul>
        <li><a href='/logout'>Logout</a></li>
        <li><a href='/'>Orders</a></li>
      </ul>
    </nav>


    <form action="/sessions" method='post'>
      <input type="hidden" name='_method' value='delete'>
      <input type="submit" value='log-out'>
    </form>



    <script type="text/template" id="form-template">

        <input type='text' id='<%= id %>' placeholder='Enter food name'>

        <input type='text' id='cents' placeholder='Enter cents'>

        <input type='text' id='description' placeholder='Enter description'>

        <button id='submit-<%= id %>'><%= submitValue %></button>

      </script>


  </body>
</html>
